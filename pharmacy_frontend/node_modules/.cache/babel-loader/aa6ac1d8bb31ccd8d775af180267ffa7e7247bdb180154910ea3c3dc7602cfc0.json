{"ast":null,"code":"import { IconUser } from \"@arco-design/web-vue/es/icon\";\nimport { useRoute } from \"vue-router\";\nimport { onMounted, ref } from \"vue\";\nexport default {\n  name: \"AdminInfo\",\n  components: {\n    IconUser\n  },\n  setup() {\n    const route = useRoute();\n    const accout = ref(route.query.AdminId);\n    const columns = [{\n      title: \"药品名称\",\n      dataIndex: \"medicineName\"\n    }, {\n      title: \"药品序号\",\n      dataIndex: \"m_id\",\n      sortable: {\n        sortDirections: [\"ascend\", \"descend\"]\n      }\n    }, {\n      title: \"药品库存\",\n      dataIndex: \"m_amount\"\n    }, {\n      title: \"药品有效期\",\n      dataIndex: \"m_expire\"\n    }];\n    const data = ref([]);\n    const visible = ref(false);\n    const handleClick = () => {\n      visible.value = true;\n    };\n    const handleOk = () => {\n      visible.value = false;\n    };\n    const handleCancel = () => {\n      visible.value = false;\n    };\n    const form = ref({\n      medicineName: \"\",\n      medicineAmount: \"\",\n      medicineExpire: \"\",\n      medicinePrice: \"\",\n      medicineType: \"\",\n      medicineDesc: \"\",\n      medicinePic: \"\",\n      medicineId: \"\"\n    });\n    const handleSubmit = data => {\n      data.value = data;\n      // TODO: 把数据提交到后端和数据库\n    };\n\n    const options = ref([\"奥利司他胶囊\", \"杰士邦\", \"杜蕾斯\"]);\n    onMounted(() => {\n      accout.value = route.query.AdminId;\n      data.value = [\n      //示例:\n      {\n        medicineName: \"阿莫西林\",\n        m_id: 2,\n        m_amount: 10,\n        m_expire: \"2022-01-01\"\n      }, {\n        medicineName: \"阿莫西林2\",\n        m_id: 1,\n        m_amount: 100,\n        m_expire: \"2022-01-01\"\n      }, {\n        medicineName: \"阿莫西林3\",\n        m_id: 4,\n        m_amount: 10,\n        m_expire: \"2022-01-01\"\n      }, {\n        medicineName: \"阿莫西林4\",\n        m_id: 3,\n        m_amount: 10,\n        m_expire: \"2022-01-01\"\n      }\n      //TODO: 从数据库获取数据\n      ];\n    });\n\n    return {\n      accout,\n      data,\n      columns,\n      visible,\n      handleClick,\n      handleCancel,\n      handleOk,\n      form,\n      handleSubmit,\n      options\n    };\n  }\n};","map":{"version":3,"names":["IconUser","useRoute","onMounted","ref","name","components","setup","route","accout","query","AdminId","columns","title","dataIndex","sortable","sortDirections","data","visible","handleClick","value","handleOk","handleCancel","form","medicineName","medicineAmount","medicineExpire","medicinePrice","medicineType","medicineDesc","medicinePic","medicineId","handleSubmit","options","m_id","m_amount","m_expire"],"sources":["/Users/alanzeng/Documents/GitHub/SE_Pharmacy/pharmacy_frontend/src/views/Admin/Administer.vue"],"sourcesContent":["<template>\n  <a-space>\n    <a-avatar :style=\"{ backgroundColor: '#3370ff' }\">\n      <IconUser />\n    </a-avatar>\n    <a-space>\n      <h1>管理员ID: {{ accout }}</h1>\n      <a-button type=\"primary\">\n        <router-link to=\"/home\">退出登陆</router-link>\n      </a-button>\n    </a-space>\n    <a-space\n      :style=\"{\n        width: '320px',\n        display: 'flex',\n        justifyContent: 'space-between',\n      }\"\n    >\n      <a-button type=\"primary\" @click=\"handleClick\"> 添加药品 </a-button>\n      <a-modal\n        v-model:visible=\"visible\"\n        @submit=\"handleSubmit()\"\n        @cancel=\"handleCancel\"\n      >\n        <template #title> 添加药品 </template>\n        <a-form :model=\"form\" :style=\"{ width: '100%' }\">\n          <a-form-item field=\"medicineName\" label=\"药品名称\">\n            <a-select\n              :options=\"options\"\n              :style=\"{ width: '100%' }\"\n              placeholder=\"请选择要添加的药品\"\n              :allow-search=\"{ retainInputValue: true }\"\n            />\n          </a-form-item>\n          <a-form-item field=\"medicineAmount\" label=\"药品数量\">\n            <a-input\n              v-model=\"form.medicneAmount\"\n              placeholder=\"请输入药品数量\"\n            />\n          </a-form-item>\n          <a-form-item field=\"medicineExpired\" label=\"药品有效期\">\n            <a-date-picker style=\"width: 200px\" />\n          </a-form-item>\n        </a-form>\n      </a-modal>\n    </a-space>\n  </a-space>\n  <a-table :columns=\"columns\" :data=\"data\" />\n</template>\n\n<script>\nimport { IconUser } from \"@arco-design/web-vue/es/icon\";\nimport { useRoute } from \"vue-router\";\nimport { onMounted, ref } from \"vue\";\n\nexport default {\n  name: \"AdminInfo\",\n  components: {\n    IconUser,\n  },\n  setup() {\n    const route = useRoute();\n    const accout = ref(route.query.AdminId);\n    const columns = [\n      {\n        title: \"药品名称\",\n        dataIndex: \"medicineName\",\n      },\n      {\n        title: \"药品序号\",\n        dataIndex: \"m_id\",\n        sortable: {\n          sortDirections: [\"ascend\", \"descend\"],\n        },\n      },\n      {\n        title: \"药品库存\",\n        dataIndex: \"m_amount\",\n      },\n      {\n        title: \"药品有效期\",\n        dataIndex: \"m_expire\",\n      },\n    ];\n    const data = ref([]);\n    const visible = ref(false);\n\n    const handleClick = () => {\n      visible.value = true;\n    };\n    const handleOk = () => {\n      visible.value = false;\n    };\n    const handleCancel = () => {\n      visible.value = false;\n    };\n    const form = ref({\n      medicineName: \"\",\n      medicineAmount: \"\",\n      medicineExpire: \"\",\n      medicinePrice: \"\",\n      medicineType: \"\",\n      medicineDesc: \"\",\n      medicinePic: \"\",\n      medicineId: \"\",\n    });\n\n    const handleSubmit = (data) => {\n      data.value = data;\n      // TODO: 把数据提交到后端和数据库\n    };\n\n    const options = ref([\"奥利司他胶囊\", \"杰士邦\", \"杜蕾斯\"]);\n\n    onMounted(() => {\n      accout.value = route.query.AdminId;\n      data.value = [\n        //示例:\n        {\n          medicineName: \"阿莫西林\",\n          m_id: 2,\n          m_amount: 10,\n          m_expire: \"2022-01-01\",\n        },\n        {\n          medicineName: \"阿莫西林2\",\n          m_id: 1,\n          m_amount: 100,\n          m_expire: \"2022-01-01\",\n        },\n        {\n          medicineName: \"阿莫西林3\",\n          m_id: 4,\n          m_amount: 10,\n          m_expire: \"2022-01-01\",\n        },\n        {\n          medicineName: \"阿莫西林4\",\n          m_id: 3,\n          m_amount: 10,\n          m_expire: \"2022-01-01\",\n        },\n        //TODO: 从数据库获取数据\n      ];\n    });\n\n    return {\n      accout,\n      data,\n      columns,\n      visible,\n      handleClick,\n      handleCancel,\n      handleOk,\n      form,\n      handleSubmit,\n      options,\n    };\n  },\n};\n</script>\n\n<style scoped></style>\n"],"mappings":"AAmDA,SAASA,QAAO,QAAS,8BAA8B;AACvD,SAASC,QAAO,QAAS,YAAY;AACrC,SAASC,SAAS,EAAEC,GAAE,QAAS,KAAK;AAEpC,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,UAAU,EAAE;IACVL;EACF,CAAC;EACDM,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIN,QAAQ,CAAC,CAAC;IACxB,MAAMO,MAAK,GAAIL,GAAG,CAACI,KAAK,CAACE,KAAK,CAACC,OAAO,CAAC;IACvC,MAAMC,OAAM,GAAI,CACd;MACEC,KAAK,EAAE,MAAM;MACbC,SAAS,EAAE;IACb,CAAC,EACD;MACED,KAAK,EAAE,MAAM;MACbC,SAAS,EAAE,MAAM;MACjBC,QAAQ,EAAE;QACRC,cAAc,EAAE,CAAC,QAAQ,EAAE,SAAS;MACtC;IACF,CAAC,EACD;MACEH,KAAK,EAAE,MAAM;MACbC,SAAS,EAAE;IACb,CAAC,EACD;MACED,KAAK,EAAE,OAAO;MACdC,SAAS,EAAE;IACb,CAAC,CACF;IACD,MAAMG,IAAG,GAAIb,GAAG,CAAC,EAAE,CAAC;IACpB,MAAMc,OAAM,GAAId,GAAG,CAAC,KAAK,CAAC;IAE1B,MAAMe,WAAU,GAAIA,CAAA,KAAM;MACxBD,OAAO,CAACE,KAAI,GAAI,IAAI;IACtB,CAAC;IACD,MAAMC,QAAO,GAAIA,CAAA,KAAM;MACrBH,OAAO,CAACE,KAAI,GAAI,KAAK;IACvB,CAAC;IACD,MAAME,YAAW,GAAIA,CAAA,KAAM;MACzBJ,OAAO,CAACE,KAAI,GAAI,KAAK;IACvB,CAAC;IACD,MAAMG,IAAG,GAAInB,GAAG,CAAC;MACfoB,YAAY,EAAE,EAAE;MAChBC,cAAc,EAAE,EAAE;MAClBC,cAAc,EAAE,EAAE;MAClBC,aAAa,EAAE,EAAE;MACjBC,YAAY,EAAE,EAAE;MAChBC,YAAY,EAAE,EAAE;MAChBC,WAAW,EAAE,EAAE;MACfC,UAAU,EAAE;IACd,CAAC,CAAC;IAEF,MAAMC,YAAW,GAAKf,IAAI,IAAK;MAC7BA,IAAI,CAACG,KAAI,GAAIH,IAAI;MACjB;IACF,CAAC;;IAED,MAAMgB,OAAM,GAAI7B,GAAG,CAAC,CAAC,QAAQ,EAAE,KAAK,EAAE,